  <script>
    var ctype = `<%= clientType %>`;
    var dhref = `/${ctype}/dashboard`
    if(ctype == 'admin') dhref = '/superuser/dashboard';
    document.querySelectorAll('.dshadd').forEach(link => {
      link.href = dhref;
    })
    if(ctype === '') document.querySelectorAll('.dshadd').forEach(link => {
      link.style.display = 'none';
    })
  </script>
  <script>
    window.addEventListener('load', () => {
      var preloader = document.querySelector('.preloader');
      setTimeout(() => {
        preloader.classList.add('preload-finish');
      }, 1000);
    });
    document.addEventListener('DOMContentLoaded', function () {
      var homenav = document.querySelectorAll('.sidenav');
      var instances = M.Sidenav.init(homenav, {});
    });
    var navWrap = document.querySelector('.brand-logo');
    window.addEventListener('resize', () => {
      if (window.screen.width < '994') {
        navWrap.classList.remove('ml-5');
      } else {
        if (window.screen.width >= '994') {
          navWrap.classList.add('ml-5');
        }
      }
    });
    if (window.screen.width < '994') {
        navWrap.classList.remove('ml-5');
      } 
  </script>
